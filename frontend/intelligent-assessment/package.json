{
  "name": "neurolearns-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered intelligent assessment platform for Chennai students and teachers",
  "author": {
    "name": "neurolearns Team",
    "email": "dev@neurolearns.com",
    "url": "https://neurolearns.com"
  },
  "license": "MIT",
  "homepage": "https://neurolearns.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/neurolearns/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/neurolearns/frontend/issues"
  },
  "keywords": [
    "assessment",
    "education",
    "ai",
    "quiz",
    "learning",
    "chennai",
    "tamil-nadu",
    "react",
    "vite",
    "pwa"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 2 versions",
      "Chrome >= 88",
      "Firefox >= 85",
      "Safari >= 14",
      "Edge >= 88"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview --host",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "rimraf dist node_modules package-lock.json && npm install",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "lhci autorun",
    "security-audit": "npm audit --audit-level high",
    "dependency-check": "depcheck",
    "outdated": "npm outdated",
    "size-check": "size-limit",
    "pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.8.4",
    "@mui/material": "^5.14.18",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/system": "^5.14.18",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.5",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.3.3",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "react-toastify": "^9.1.3",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^4.0.11",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "recharts": "^2.8.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-intersection-observer": "^9.5.3",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "react-virtualized-auto-sizer": "^1.0.20",
    "fuse.js": "^7.0.0",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.7",
    "workbox-window": "^7.0.0",
    "idb": "^7.1.1",
    "browser-image-compression": "^2.0.2",
    "react-webcam": "^7.1.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "file-saver": "^2.0.5",
    "crypto-js": "^4.2.0",
    "ua-parser-js": "^1.0.37",
    "i18next": "^23.7.6",
    "react-i18next": "^13.5.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/lodash-es": "^4.17.12",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/react-window": "^1.8.8",
    "@types/file-saver": "^2.0.7",
    "@types/crypto-js": "^4.2.1",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-bundle-analyzer": "^0.7.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-eslint": "^1.8.1",
    "typescript": "^5.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-prettier": "^10.0.0",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jsdom": "^23.0.1",
    "@playwright/test": "^1.40.1",
    "playwright": "^1.40.1",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "storybook": "^7.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "semantic-release": "^22.0.8",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.3",
    "@lhci/cli": "^0.12.0",
    "depcheck": "^1.4.7",
    "rimraf": "^5.0.5",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "start-server-and-test": "^2.0.3",
    "size-limit": "^10.0.2",
    "@size-limit/preset-app": "^10.0.2",
    "pwa-asset-generator": "^6.3.1",
    "@vite-pwa/assets-generator": "^0.0.11",
    "sharp": "^0.32.6",
    "@types/sharp": "^0.32.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test:run && npm run type-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "500 KB",
      "webpack": false,
      "running": false
    },
    {
      "path": "dist/assets/*.css",
      "limit": "100 KB"
    }
  ],
  "volta": {
    "node": "18.18.2",
    "npm": "10.2.3"
  },
  "packageManager": "npm@10.2.3"
}
